{
  "name": "newmuweb",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lingui/react": "^2.7.0",
    "ajv": "^6.5.4",
    "axios": "^0.18.0",
    "history": "^4.6.3",
    "jquery": "^3.3.1",
    "mdbootstrap": "^4.5.11",
    "mdbreact": "^4.8.0",
    "mini-css-extract-plugin": "0.4.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "react": "^16.6.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-country-region-selector": "^1.4.1",
    "react-datepicker": "^1.6.0",
    "react-dom": "^16.6.0",
    "react-facebook-login": "^4.1.0",
    "react-html5video": "^2.5.1",
    "react-image-gallery": "^0.8.11",
    "react-infinite-calendar": "^2.3.1",
    "react-js-google-maps": "^1.5.2",
    "react-phone-number-input": "^2.2.8",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.3",
    "react-select": "^2.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "sass-true": "^4.0.0",
    "video-react": "^0.13.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "compile": "lingui compile"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@lingui/cli": "^2.7.0",
    "@lingui/macro": "^2.7.0",
    "babel-core": "^7.0.0-bridge.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "description": "This is the new branch for Muzikol web application. This is done because, we are switching from react 1.x to react 2.0.5",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsaffi/newmuweb.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tsaffi/newmuweb/issues"
  },
  "homepage": "https://github.com/tsaffi/newmuweb#readme"
}
